<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="git.simple.mybatis.dao.IStudentDao">
    <select id="findAll" resultType="git.simple.mybatis.model.Student">
        select * from student_info
    </select>

    <select id="findById" parameterType="int" resultType="git.simple.mybatis.model.Student">
        select * from student_info where id = #{id}
    </select>
</mapper>